package excel

import (
    "github.com/tealeg/xlsx"
    "testing"
)

func TestConnectToRouter67(t *testing.T) {
    // Create a new Excel file
    file := xlsx.NewFile()
    _, err := file.AddSheet("Nping Output")
    if err != nil {
        t.Fatalf("Error creating Excel sheet: %v", err)
    }

    // Save the Excel file
    filePath := "/home/tcs/sample/ondatra/debug/acl/excel/nping67_output.xlsx"
    if err := file.Save(filePath); err != nil {
        t.Fatalf("Error saving Excel file: %v", err)
    }

    // Print the path at the end of the output
    t.Logf("Path for excel: %v", filePath)
}
